{
  "name": "ivaldi-italian-food-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/bin/www.ts",
    "build": "tsc --project ./",
    "prestart": "rimraf build && yarn run build",
    "start": "cross-env NODE_ENV=production node ./build/src/bin/www.js",
    "serve": "node ./build/src/bin/www.js",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:coverage": "nyc npm run test"
  },
  "dependencies": {
    "apollo-server-express": "^3.9.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mocha": "^10.0.0",
    "morgan": "~1.9.1",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "nodemon": "^2.0.18",
    "nyc": "^15.1.0",
    "supertest": "^6.2.3",
    "typescript": "^4.7.4"
  }
}
